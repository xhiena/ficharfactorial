{
    "name": "factorial-time-tracker",
    "version": "1.0.0",
    "description": "Playwright automation for Factorial app time tracking",
    "main": "dist/index.js",
    "type": "module",
    "scripts": {
        "build": "tsc",
        "start": "npm run build && node dist/index.js",
        "dev": "tsx src/index.ts",
        "test": "playwright test",
        "install:browsers": "playwright install",
        "setup": "npm install && npm run install:browsers"
    },
    "keywords": [
        "playwright",
        "automation",
        "factorial",
        "time-tracking"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "@playwright/test": "^1.40.0",
        "dotenv": "^16.3.1",
        "winston": "^3.11.0",
        "commander": "^11.1.0",
        "chalk": "^5.3.0"
    },
    "devDependencies": {
        "@types/node": "^20.8.0",
        "typescript": "^5.2.2",
        "tsx": "^3.14.0"
    },
    "engines": {
        "node": ">=18.0.0"
    }
}